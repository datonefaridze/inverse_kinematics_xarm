<?xml version="1.0"?>
<robot name="custom_robot">

  <!-- Base Link -->
  <link name="base_link">
  </link>

  <!-- Joint 1 -->
  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 0.055"/>
    <limit lower="-1.5707" upper="1.5707" effort="30" velocity="1.0"/>
  </joint>

  <link name="link1">
  </link>

  <!-- Joint 2 -->
  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 0.02" rpy="-1.57 0 -1.57"/>
    <limit lower="-1.5707" upper="1.5707" effort="30" velocity="1.0"/>
  </joint>

  <link name="link2">
  </link>

  <!-- Joint 3 -->
  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.10 0 0" rpy="-3.1415 0 0"/>
    <limit lower="-1.5707" upper="1.5707" effort="30" velocity="1.0"/>
  </joint>

  <link name="link3">
  </link>

  <!-- Joint 4 -->
  <joint name="joint4" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.095 0 0" rpy="-3.1415 0 0"/>
    <limit lower="-1.5707" upper="1.5707" effort="30" velocity="1.0"/>
  </joint>

  <link name="link4">
  </link>

  <!-- Joint 5 -->
  <!-- <joint name="joint5" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 0.014" rpy="-1.57 0 -1.57"/>
    <limit lower="-1.5707" upper="1.5707">
  </joint>
  
  <link name="link5">
  </link> -->

</robot>
