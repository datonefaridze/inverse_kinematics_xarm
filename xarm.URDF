<?xml version="1.0"?>
<robot name="custom_robot">
  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="5.5 5.5 5.5"/>
      </geometry>
    </visual>
  </link>

  <!-- Joint 1 -->
  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 5.5"/>
    <limit lower="-1.5707" upper="1.5707" effort="30" velocity="1.0"/>
  </joint>
  <link name="link1">
    <visual>
      <geometry>
        <box size="2 2 2"/>
      </geometry>
    </visual>
  </link>

  <!-- Joint 2 -->
  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 2"/>
    <limit lower="-1.5707" upper="1.5707" effort="30" velocity="1.0"/>
  </joint>
  <link name="link2">
    <visual>
      <geometry>
        <box size="2 2 10"/>
      </geometry>
    </visual>
  </link>

  <!-- Joint 3 -->
  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 10"/>
    <limit lower="-1.5707" upper="1.5707" effort="30" velocity="1.0"/>
  </joint>
  <link name="link3">
    <visual>
      <geometry>
        <box size="2 2 9.5"/>
      </geometry>
    </visual>
  </link>

  <!-- Joint 4 -->
  <joint name="joint4" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 9.5"/>
    <limit lower="-1.5707" upper="1.5707" effort="30" velocity="1.0"/>
  </joint>
  <link name="link4">
    <visual>
      <geometry>
        <box size="2 2 4"/>
      </geometry>
    </visual>
  </link>


  <!-- Joint 5 -->
<joint name="joint5" type="revolute">
  <parent link="link4"/>
  <child link="link5"/>
  <axis xyz="1 0 0"/>
  <origin xyz="0 0 4"/>
  <limit lower="-1.5707" upper="1.5707" effort="30" velocity="1.0"/>
</joint>
<link name="link5">
  <visual>
    <geometry>
      <box size="2 2 4"/>
    </geometry>
  </visual>
</link>



</robot>
